<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.jdt.core.classpathContainerInitializer">
      <classpathContainerInitializer
            class="com.google.gwt.eclipse.core.GWTRuntimeContainerInitializer"
            id="com.google.gwt.eclipse.core.GWT_CONTAINER">
      </classpathContainerInitializer>
   </extension>
   <extension
         id="gwtNature"
         name="GWT Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="com.google.gwt.eclipse.core.nature.GWTNature">
         </run>
      </runtime>
      <requires-nature
            id="org.eclipse.jdt.core.javanature">
      </requires-nature>
   </extension>   
</plugin>
